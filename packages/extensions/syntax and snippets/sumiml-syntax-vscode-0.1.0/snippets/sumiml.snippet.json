{
  "SumiML directive": {
		"prefix": "sumiml",
		"body": [
			"<SumiML version=\"1.0\" />",
      "<Frontmatter>",
      "",
      "</Frontmatter>",
      "<Document>",
      "",
      "</Document>"
		],
		"description": "starts writing SumiML document"
	},
  "SumiML Section": {
    "prefix": "section",
    "body": [
      "<Section title=\"$1\">",
      "$0",
      "</Section>"
    ],
    "description": "add a section"
  },
  "SumiML Subsection": {
    "prefix": "sub",
    "body": [
      "<Subsection title=\"$1\">",
      "$0",
      "</Subsection>"
    ],
    "description": "add a subsection"
  },
  "SumiML Subsubsection": {
    "prefix": "ssub",
    "body": [
      "<Subsubsection title=\"$1\">",
      "$0",
      "</Subsubsection>"
    ],
    "description": "add a subsubsection"
  }
}